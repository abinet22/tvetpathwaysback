</head>

<body class="nav-md">
	<div class="container body">
		<div class="main_container">
            <%- include ('./partials/sidebar') %>
            <!-- top navigation -->
            <%- include ('./partials/header') %>
			<!-- /top navigation -->

			<!-- page content -->
			<div class="right_col" role="main">
                
				<div class="">
					<div class="container-fluid">
                        <div class="page-header min-height-100 border-radius-xl mt-4" style="background-image: url('../../../../assets/img/curved-images/sds.jpg'); background-position-y: 50%;">
                          <span class="mask  opacity-6"></span>
                          <div >
                            <nav aria-label="breadcrumb">
                              <ol class="breadcrumb">
                                <li class="breadcrumb-item"><a href="#">Home</a></li>
                                <li class="breadcrumb-item"><a href="#">Manage Configuration</a></li>
                                <li class="breadcrumb-item active" aria-current="page">Create Job Category</li>
                              </ol>
                            </nav>
                          </div>
                        </div>
                      
                      </div>
					<div class="clearfix"></div>
					<div class="row">
                        <%- include ('./partials/messages')%>
						<div class="col-md-12 col-sm-12 ">
							<div class="x_panel">
								<div class="x_title">
									<h2>Create Job Category</h2>
									
									<div class="clearfix"></div>
								</div>
								
								<div class="x_content">
									
									<br />
									
									<ul class="nav nav-tabs justify-content-end bar_tabs" id="myTab" role="tablist">
								
										<li class="nav-item active">
										  <a class="nav-link  active" id="contact-tab" data-toggle="tab" href="#contact1" role="tab" aria-controls="contact" aria-selected="false">Create Job Main Category</a>
										</li>
										<li class="nav-item">
											<a class="nav-link" id="contact3-tab" data-toggle="tab" href="#contact3" role="tab" aria-controls="contact3" aria-selected="false">Create Job Sub Category</a>
										  </li>
										<li class="nav-item">
											<a class="nav-link" id="contact2-tab" data-toggle="tab" href="#contact2" role="tab" aria-controls="contact2" aria-selected="false">Config List</a>
										  </li>
									  </ul>
									  <div class="tab-content" id="myTabContent">
										
										<div class="tab-pane fade show active" id="contact1" role="tabpanel" aria-labelledby="contact-tab">
                                            <form id="demo-form2" data-parsley-validate class="form-horizontal form-label-left" action="/config/addmainjobcategory" method="post">

												<div class="item form-group">
													<label class="col-form-label col-md-3 col-sm-3 label-align" for="full-name">Job Main Category <span class="required">*</span>
													</label>
													<div class="col-md-6 col-sm-6 ">
														<input type="text" id="mainjobcategory" name="mainjobcategory"  required="required" class="form-control ">
													</div>
												</div>
											
												<div class="ln_solid"></div>
												<div class="item form-group">
													<div class="col-md-6 col-sm-6 offset-md-3">
														
														<button type="submit" class="btn btn-success btn-block">Submit</button>
													</div>
												</div>
		
											</form>
										</div>
										<div class="tab-pane fade" id="contact3" role="tabpane" aria-labelledby="contact3-tab">
                                            <form id="demo-form2" data-parsley-validate class="form-horizontal form-label-left" action="/config/addsubjobcategory" method="post">
												<div class="item form-group">
													<label class="col-form-label col-md-3 col-sm-3 label-align" for="user-roll">Job Main Category<span class="required">*</span>
													</label>
													<div class="col-md-6 col-sm-6 ">
														<select class="form-control" name="mainjobcategory" id="mainjobcategory">
															<option value="0">Select Job Main Category</option>
															<% jobmaincategory.forEach(function (row){%>
																<option value="<%= row.catid %>"><%= row.catname %></option>
														    <%})%>
															
														</select></div>
												</div>
												<div class="item form-group">
													<label class="col-form-label col-md-3 col-sm-3 label-align" for="full-name">Job Sub Category <span class="required">*</span>
													</label>
													<div class="col-md-6 col-sm-6 ">
														<input type="text" id="subcatname" name="subcatname"  required="required" class="form-control ">
													</div>
												</div>
											
												<div class="ln_solid"></div>
												<div class="item form-group">
													<div class="col-md-6 col-sm-6 offset-md-3">
														
														<button type="submit" class="btn btn-success btn-block">Submit</button>
													</div>
												</div>
		
											</form>
										  </div>
										<div class="tab-pane fade" id="contact2" role="tabpane" aria-labelledby="contact2-tab">
                                            <div class="table-responsive">
                                                <table class="table table-striped jambo_table bulk_action">
                                                  <thead>
                                                    <tr class="headings">
														<th class="column-title">#</th>
                                                      <th class="column-title">Job Main/Sub Category Name </th>
                                                     
                                                      <th class="column-title">Is Active</th>

													  <th class="column-title">Action ToDo</th>
                                                      
                                                    </tr>
                                                  </thead>
                          
                                                  <tbody>
                                                  
                                                 <% if(jobmaincategory.length >0){%>
													<% var i= 0%>
                                                 <% jobmaincategory.forEach(function (row){%>
													<% i++ %>
                                                  <tr class="even pointer">
                                                     <td><%= i %></td>
                                                 
                                                      <td class=" "><%= row.catname %></td>
                                                  
                                                     
                                                      <td class=" last">
														<% if(row.is_active ==="Yes"){%>
															<button class="btn btn-outline-warning btn-sm ">Diactivate</button>
															
														 <% }else{%>
															<button class="btn btn-outline-success btn-sm ">Activate</button>
														
														  <%}%>
                                                         
                                                      </td>
													  <td>
														<form action="/config/deletemainjobcategory/<%= row.catid %>" method="post">
															<button class="btn btn-outline-danger btn-sm ">Delete</button>
														</form>
													  </td>
                                                    </tr>
													
                                                 <% })}else{%>
                                                  <tr>
                                                      <div class="alert alert-warning alert-dismissible " role="alert">
                                                          <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">Ã—</span>
                                                          </button>
                                                          <strong>Error!</strong> No Job Config Found!
                                                        </div>
                                                  </tr>
                                                 <% }%>
												 <% jobsubcategory.forEach(function (row){%>
													<% i++ %>
												  <tr class="even pointer">
													 <td><%= i %></td>
												 
													  <td class=" "><%= row.subcatname %></td>
												  
													 
													  <td class=" last">
														  <% if(row.is_active ==="Yes"){%>
															<button class="btn btn-outline-warning btn-sm ">Diactivate</button>
															
														 <% }else{%>
															<button class="btn btn-outline-success btn-sm ">Activate</button>
														
														  <%}%>
														 
													  </td>
													  <td>
														<form action="/config/deletsubjobcategory/<%= row.subcatid %>" method="post">
															<button class="btn btn-outline-danger btn-sm ">Delete</button>
														</form>
													  </td>
													</tr>
												 <% })%>
                                                  </tbody>
                                                </table>
                                              </div>
										  </div>

									  </div>
									
								</div>
							</div>
						</div>
					</div>

				</div>
			</div>
			<!-- /page content -->

			<!-- footer content -->
		<%- include ('./partials/footer')%>
			<!-- /footer content -->
		</div>
	</div>

	<!-- jQuery -->
	<script src="../../../../vendors/jquery/dist/jquery.min.js"></script>
